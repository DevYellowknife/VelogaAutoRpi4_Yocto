[Unit]
Description=Cluster GUI Application
After=OBDManager.service CANManager.service

[Service]
Environment="QT_GSTREAMER_PLAYBIN_AUDIOSINK=fakesink" "QT_QPA_PLATFORM=eglfs" "QT_QPA_EGLFS_ALWAYS_SET_MODE=\"1\"" "QT_QPA_EGLFS_KMS_CONFIG=/etc/eglfs_kms.config" "QT_WAYLAND_SHELL_INTEGRATION=xdg-shell"
ExecStart=-/opt/VelogaAuto/VelogaAuto
Restart=on-failure

[Install]
WantedBy=multi-user.target